{
  "name": "xiaohongshu-parser",
  "version": "1.0.0",
  "description": "Parse xiaohongshu and douyin content",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "build:clean": "rm -rf dist && npm run build",
    "dev": "tsc --watch",
    "test": "node test-video-validator.js",
    "test:types": "node test-types.js",
    "test:douyin": "node test-douyin-parser.js",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "generate-types": "tsc --declaration --emitDeclarationOnly",
    "lint": "tsc --noEmit && echo 'TypeScript check passed'",
    "clean": "rm -rf dist types/generated",
    "deploy": "npm run build && wrangler deploy",
    "start": "node dist/index.js",
    "migration:backup": "find src -name '*.js' -exec cp {} {}.backup \\;",
    "migration:restore": "find src -name '*.backup' -exec sh -c 'mv \"$1\" \"${1%.backup}\"' _ {} \\;"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.820.0",
    "@aws-sdk/lib-storage": "^3.820.0",
    "@aws-sdk/s3-request-presigner": "^3.820.0",
    "@notionhq/client": "^3.1.3",
    "axios": "^1.9.0",
    "dotenv": "^16.5.0",
    "itty-router": "^4.2.2",
    "puppeteer": "^24.9.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.0",
    "node-fetch": "^2.7.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "wrangler": "^4.19.1"
  }
}
